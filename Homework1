#include <iostream>
#include <string>
using namespace std;
int main()
{
	string Name = "Tanakrit";
	float Score1 = 98 , Score2 = 95 , GPA = 3.95;
	char Grade = 'A';

	cout << "My name is : " << Name << endl;
	cout << "My scores 1 : " << Score1 << "points"<< endl;
	cout << "My scores 2 : " << Score2 << "points"<< endl;
	cout << "Total : " << Score1+Score2 << "points" <<endl;
	cout << "Grade : " << Grade <<endl;
	cout << "GPA : "<< GPA << endl;
	return (0);

}
